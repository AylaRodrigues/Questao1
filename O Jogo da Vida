# Questao1
Garotes de Programa- Ayla Rodrigues de Moura, Douglas Mareli, João Spala, Sofia Kitaeva - Questão 1

#include <stdio.h>
#include <stdlib.h>

int main () {
	
	int quadrado[30][30];
	
	for(int i = 0; i < 30; i++)
	{
		for(int j = 0; j< 30; j++)
		{
			quadrado[i][j] = 0;
		}
	}
	
	//opção de escolha
	int x,y; //linha e coluna
	while(x != -1 && y != -1)
	{
		scanf("%d %d",&x,&y);
		quadrado[x][y] = 1;
	}
	
	//printar o tabuleiro
	for(int y = 0; y < 30; y++)
	{
		for(int z = 0; z < 30; z++)
		{
			if(quadrado[y][z] == 1)
			{
				printf("0");
			}else if(quadrado[y][z] == 0)
			{
				printf(" ");
			}
				
		}
		printf("\n");
	}
	
	return 0;
}
