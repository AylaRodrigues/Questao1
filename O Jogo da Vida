#include <stdio.h>
#include <stdlib.h>

int main () {

	char start;
	int quadrado[30][30];

	printf("Para iniciação randômica digite 'r', para selecionar as células digite 'c'\n");
	scanf("%s", &start);
	
	for(int i = 0; i < 30; i++)
	{
		for(int j = 0; j< 30; j++)
		{
			quadrado[i][j] = 0;
		}
	}

	if (start== 'r')
	{
		int espacos;
		int rd;
		int l,c;
		printf("Quantas células vivas?\n");
		scanf("%d", &espacos);
		rd = espacos;
		
		for(int i = 0; i < espacos; i++)
		{
			l= rand() %29;
			c= rand() %29;
			quadrado[l][c]=1;
		}
	}
	
	for(int y = 0; y < 30; y++)
	{
		for(int z = 0; z < 30; z++)
		{
			if(quadrado[y][z] == 1)
			{
				printf("0");
			}else if(quadrado[y][z] == 0)
			{
				printf(" ");
			}
				
		}
		printf("\n");
	}
	
	return 0;	
}
